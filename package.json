{
  "name": "lmrc-noticeboard",
  "version": "1.0.0",
  "description": "Lake Macquarie Rowing Club Digital Noticeboard",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "scrape": "node scraper/noticeboard-scraper.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:react\"",
    "dev:server": "nodemon server.js",
    "dev:react": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "setup": "node scripts/setup.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:scraper:old": "node scripts/test-scraper.js"
  },
  "keywords": [
    "noticeboard",
    "rowing",
    "digital-signage"
  ],
  "author": "LMRC",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "concurrently": "^8.2.2",
    "jsdom": "^27.3.0",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "terser": "^5.44.0",
    "vite": "^7.1.10",
    "vitest": "^4.0.16"
  }
}
